        "@dataclass\n",
        "class PersonalityTraits:\n",
        "    personality_type: str\n",
        "    communication_style: str\n",
        "    decision_making: str\n",
        "    fashion_style: List[str]\n",
        "    color_preferences: List[str]\n",
