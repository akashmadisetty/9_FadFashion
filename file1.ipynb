        "            sustainability_focus=random.choice([\"high\", \"medium\", \"low\"]),\n",
        "            interests=random.sample([\"local culture\", \"global trends\", \"arts\", \"technology\"], k=2),\n",
        "            values=random.sample([\"tradition\", \"innovation\", \"sustainability\", \"quality\"], k=2)\n",
        "        )\n",
        "\n",
        "        agent = Agent(\n",
        "            name=f\"person_{i+1}\",\n",
        "            age=random.randint(demographics.target_age_range[0], demographics.target_age_range[1]),\n",
        "            nationality=demographics.target_nationality,\n",
        "            occupation=\"Student\" if random.random() < 0.3 else \"Professional\",\n",
